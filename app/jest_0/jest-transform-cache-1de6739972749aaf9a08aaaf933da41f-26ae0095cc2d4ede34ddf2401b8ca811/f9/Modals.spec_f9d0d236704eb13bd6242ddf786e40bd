"use strict";

var _interopRequireDefault = require("/home/oem/Desktop/Prolike/dilly-dally/app/node_modules/@babel/runtime-corejs2/helpers/interopRequireDefault");

var _vue = _interopRequireDefault(require("vue"));

var _testUtils = require("@vue/test-utils");

var _bootstrapVue = _interopRequireDefault(require("bootstrap-vue"));

var _Modals = _interopRequireDefault(require("@/views/notifications/Modals"));

_vue.default.use(_bootstrapVue.default);

describe('Modals.vue', () => {
  it('has a name', () => {
    expect(_Modals.default.name).toMatch('modals');
  });
  it('has a created hook', () => {
    expect(typeof _Modals.default.data).toMatch('function');
  });
  it('sets the correct default data', () => {
    expect(typeof _Modals.default.data).toMatch('function');

    const defaultData = _Modals.default.data();

    expect(defaultData.largeModal).toBe(false);
  });
  it('is Vue instance', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Modals.default);
    expect(wrapper.isVueInstance()).toBe(true);
  });
  it('is Modals', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Modals.default);
    expect(wrapper.is(_Modals.default)).toBe(true);
  });
  it('should render correct content', () => {
    const wrapper = (0, _testUtils.mount)(_Modals.default);
    expect(wrapper.find('div.card-header > div').text()).toMatch('Bootstrap Modals');
  });
  test('renders correctly', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Modals.default);
    expect(wrapper.element).toMatchSnapshot();
  });
});
