"use strict";

var _interopRequireDefault = require("/home/oem/Desktop/Prolike/dilly-dally/app/node_modules/@babel/runtime-corejs2/helpers/interopRequireDefault");

var _vue = _interopRequireDefault(require("vue"));

var _testUtils = require("@vue/test-utils");

var _bootstrapVue = _interopRequireDefault(require("bootstrap-vue"));

var _Forms = _interopRequireDefault(require("@/views/base/Forms"));

_vue.default.use(_bootstrapVue.default);

describe('Forms.vue', () => {
  it('has a name', () => {
    expect(_Forms.default.name).toMatch('forms');
  });
  it('has a created hook', () => {
    expect(typeof _Forms.default.data).toMatch('function');
  });
  it('sets the correct default data', () => {
    expect(typeof _Forms.default.data).toMatch('function');

    const defaultData = _Forms.default.data();

    expect(defaultData.show).toBe(true);
  });
  it('is Vue instance', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Forms.default);
    expect(wrapper.isVueInstance()).toBe(true);
  });
  it('is Forms', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Forms.default);
    expect(wrapper.is(_Forms.default)).toBe(true);
  });
  it('should render correct content', () => {
    const wrapper = (0, _testUtils.mount)(_Forms.default);
    expect(wrapper.find('div.card-header > div > strong').text()).toMatch('Credit Card');
  });
  test('renders correctly', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Forms.default);
    expect(wrapper.element).toMatchSnapshot();
  });
});
