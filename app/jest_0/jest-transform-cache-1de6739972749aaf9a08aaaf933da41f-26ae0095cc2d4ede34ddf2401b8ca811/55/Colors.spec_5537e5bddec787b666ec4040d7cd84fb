"use strict";

var _interopRequireDefault = require("/home/oem/Desktop/Prolike/dilly-dally/app/node_modules/@babel/runtime-corejs2/helpers/interopRequireDefault");

var _vue = _interopRequireDefault(require("vue"));

var _testUtils = require("@vue/test-utils");

var _bootstrapVue = _interopRequireDefault(require("bootstrap-vue"));

var _Colors = _interopRequireDefault(require("@/views/theme/Colors"));

_vue.default.use(_bootstrapVue.default);

describe('Colors.vue', () => {
  it('has a name', () => {
    expect(_Colors.default.name).toMatch('colors');
  });
  it('is Vue instance', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Colors.default);
    expect(wrapper.isVueInstance()).toBe(true);
  });
  it('is Colors', () => {
    const wrapper = (0, _testUtils.shallowMount)(_Colors.default);
    expect(wrapper.is(_Colors.default)).toBe(true);
  });
  it('should render correct content', () => {
    const wrapper = (0, _testUtils.mount)(_Colors.default);
    expect(wrapper.find('header.card-header > div').text()).toMatch('Theme colors');
  });
  test('renders correctly', () => {
    const wrapper = (0, _testUtils.mount)(_Colors.default);
    expect(wrapper.element).toMatchSnapshot();
  });
});
